    "paths": {
        "/api/auth/client/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Autenticazione client",
                "description": "Effettua il login per un client e restituisce un token",
                "operationId": "8dd833fca3b3f090c3b6ab76b259c039",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "client_id",
                                    "secret_id"
                                ],
                                "properties": {
                                    "client_id": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "client_1"
                                    },
                                    "secret_id": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "secret_1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Auth effettuato con successo",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string",
                                            "example": "1|abcdefghijklmnopqrstuvwxyz123456"
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "Bearer"
                                        },
                                        "expires_at": {
                                            "type": "string",
                                            "format": "date",
                                            "example": "2025-08-02T22:06:31.000000Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenziali non valide",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
